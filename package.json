{
  "name": "plain-cache",
  "version": "1.1.6",
  "description": "Plain simple, in-memory, tiny-in-size JavaScript cache library",
  "main": "build/cache.js",
  "types": "build/cache.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./build",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test:watch": "chokidar 'src/**/*.ts' -c 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tnhu/plain-cache.git"
  },
  "keywords": [
    "cache",
    "plain cache",
    "caching",
    "simple cache"
  ],
  "author": "Tan Nhu",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "latest",
    "@types/node": "latest",
    "chokidar-cli": "latest",
    "mocha": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "dependencies": {}
}
